//弹框按钮选中样式
$('.plus-and-select .table-piece table tr td:last-child a').click(function(){
    if($(this).hasClass('change')){
        $(this).removeClass('change').text('选择');
    }else{
        $(this).addClass('change').not(this).removeClass('change').text('已选');

    }
   
})
