<script type="text/javascript">

    function difference(){
        if($(".photo-wall").hasClass("difference")){
            $(".selec-img").unbind("click");
            $(".photo-wall .photo-box .tick-box").hide();
            $(".photo-wall .photo-box img").click().unbind();
            //相册相关事件

            $(".photo-box img").mouseover(function () {
                $(".photo-close,.photo-scale").hide();
                $(this).closest(".photo-box").find(".photo-close,.photo-scale").fadeIn(200);
            });
            $(".photo-scale,.photo-close").mouseleave(function () {
                $(".photo-close,.photo-scale").fadeOut(0);
            });
            $(".photo-box").on("mouseleave",function () {
                $(".photo-close,.photo-scale").fadeOut(0);
            });
            $(".photo-close").mouseover(function () {
                $(this).closest(".photo-box").find(".photo-close,.photo-scale").fadeIn(0);
            });
            $(".photo-close").unbind("click").click(function () {
                $(this).closest(".photo-box").remove();
            });
            //相册相关事件
            $(".photo-box p i").unbind("click").click(function (e) {
                e.preventDefault();
                e.stopPropagation()
                $(this).parent().hide();
                $(this).closest(".photo-box").find(".set-new-name").show();
                $(this).closest(".photo-box").find(".set-new-name").change(function () {
                    var getVal = $(this).val();
                    $(this).closest(".photo-box").find("label").text(getVal);
                })
            });
            $(".set-new-name").unbind("click").click(function (e) {
                e.preventDefault();
                e.stopPropagation()
            });
            $(document).unbind("click").click(function () {
                $(".photo-box p").show();
                $(".set-new-name").hide();
            });

            //放大图片点击事件
            var getLength = $(".photo-box").length;
            var saveIndex;//获取index用于大图上下一张切换
            $(".photo-scale").unbind("click").click(function () {
                saveIndex = $(this).closest(".photo-box").index();
                var getUrl = $(this).parent().find("img").attr("src");
                $(".fixedBg").css("display","table");
                $("#photoLayer>img").attr("src",getUrl);
            });

            $(".fixedBg .imgBox .previous").unbind("click").click(function () {
                var saveImg = $(".photo-box:nth-child("+saveIndex+")").find("img").attr("src");
                saveIndex--;
                if(saveIndex<=0){
                    saveIndex=0
                }
                $("#photoLayer>img").attr("src",saveImg);
            });

            $(".fixedBg .imgBox .next").unbind("click").click(function () {
                var saveImg = $(".photo-box:nth-child("+(saveIndex+2)+")").find("img").attr("src");
                ++saveIndex;
                if(saveIndex>=getLength){
                    saveIndex=getLength-1;
                }
                $("#photoLayer>img").attr("src",saveImg);
            });

            //关闭放大图片层
            $(".fixedBg .close").unbind("click").click(function () {
                $(".fixedBg").fadeOut(200)
            });

        }else {
            $(".photo-wall .photo-box .tick-box").show();
            $(".photo-box img").unbind();
            $(".photo-scale").unbind();
            $(".photo-close").unbind();
            $(".selec-img").unbind("click").click(function () {
