<script type="text/javascript">
    //類型
    $(".fill-in-box .click-window").click(function () {
        $(".fill-types").show();
    });
    $(".fill-in-box .fill-types .right-select a").click(function () {
        $(".fill-types").hide();
    });
    $(".selec-id").mouseleave(function () {
        $(".fill-types").hide();
    });

    $(".qian").hover(function () {
        layer.tips('<div style="min-width:269px;padding:6px;color: #fff;font-size: 12px;position:relative"><p><span style="margin-right:10px;display: inline-block;width: 74px;">学费</span>欠费200.00元</p>' +
            '<p><span style="margin-right:10px;display: inline-block;width: 74px;">工具费</span>欠费200.00元</p>' +
            '<p><span style="margin-right:10px;display: inline-block;width: 74px;">合计</span>欠费200.00元</p></div>', $(this), {
            tips: [1, 'rgba(0,0,0,.9)'],
            area:"349px",
            time: 0
        });
    },function () {
        layer.closeAll()
    });


    //调整班级
    $(".setting-classroom").click(function () {
        layer.open({
            title: '转班',
            type: 1,
            area: '744px',
            skin: '',
            content: $('#setting-classroom')
        });
    });
    //调整班级
    $(".setting-select").click(function () {
        layer.open({
            title: '选择转班',
            type: 1,
            area: '744px',
            skin: '',
            content: $('#plus-and-select')
        });
    });




    //发通知
    $(".renewal-fee").click(function () {
        layer.open({
            title: '补费续费',
            type: 1,
            area: '800px',
            skin: '',
            content: $('#fee-layer1')
        });
    });

    //发通知
    $(".tuition-fee").click(function () {
        layer.open({
            title: '退学退费',
            type: 1,
            area: '800px',
            skin: '',
            content: $('#fee-layer')
        });
    });

    //退费状态点击事件
    $(".fill-in-box .fill-radio a").click(function () {
        $(".fill-in-box .fill-radio a").removeClass("cur");
        $(this).addClass("cur")
    });

    //添加退费项
    $(".plus-sth").click(function () {
        layer.open({
            title: '教材杂费',
            type: 1,
            area: '488px',
            skin: '',
            content: $('#plus-sth')
        });
    });






    //只看自己
    $(".see-myself").click(function () {
        if ($(this).find("i").hasClass('cur')) {
            $(this).find("i").removeClass('cur');
        } else {
            $(this).find("i").addClass('cur');

        }
    });

    //已到账
    $(".tick-end").click(function () {
        if ($(this).hasClass('cur')) {
            $(this).removeClass('cur');
        } else {
            $(this).addClass('cur');

        }
    });




</script>

