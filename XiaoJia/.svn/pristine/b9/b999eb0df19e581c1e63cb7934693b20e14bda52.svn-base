/*运营统计*/
.statistical_type{height:50px;background:#e0e1ef;color:#686593}
.statistical_type a{color:#61699b;display: block;width:113px;height:46px;float:left;border-radius: 5px 5px 0 0;line-height:46px;margin-top:4px;text-align: center}

.methods{  width: 96%;  height: 80px;  background: #f5f5fb;  color: #5d6383;  border:1px solid #eaeaf3;  margin:29px auto;}
.methods .statistical_methods{line-height:34px;margin-top:22px;width:23%;margin-left:1%;float: left;  font-size:13px;}
.methods .statistical_methods ul{width:74%;float:right;border:1px solid #d0d0e7;overflow: hidden;border-radius: 4px}
.methods .statistical_methods ul li{width:33%;float: right;text-align: center;color:#222443;cursor: pointer;background:#fff}
.methods .statistical_methods ul li:hover,.methods .statistical_methods ul li.cur{color:#fff;background: #222443}
.methods .statistical_time{line-height:36px;margin-top:22px;width:297px;margin-left:1%;float: left;  font-size:13px;height:36px}
.methods .statistical_time span{text-align: center;display: block;float: left}
.methods .statistical_time .style_change{width:75%;height:34px;position:relative;margin-top:0;float:right;border:1px solid #d0d0e7;background:#fff}
.methods .statistical_time .style_change span{float: left}
.methods .statistical_time .style_change dt{line-height:32px;width:100%;border:none; background:url(../../images/gp/icon.png) no-repeat; background-position: 100% -1806px;margin-top:1px;padding:0;font-size: 14px;text-indent:10px;}
.methods .statistical_time .style_change dt span{line-height:32px;font-size: 14px;color:#797b82}
.methods .statistical_time .style_change dd{width:100%;padding-right:1px;left:-1px;top:34px;    border: 1px solid #d0d0e7;}

.methods .enterprise_search{line-height:34px;margin-top:22px;width:20%;margin-left:13px;float: left;  font-size:14px;}
.methods .enterprise_search span{width:20%;text-align: center;display: block;float: left}
.methods .enterprise_search input{height:34px;border:1px solid #d0d0e7;border-radius: 4px;float:right;padding-left:3px;font-size: 14px;;width:75%}
.methods .statistics_custom{width:420px;font-size: 14px;float:left;color: #bdbdd6;line-height: 34px;margin:22px 1% 0;}
.methods .statistics_custom input{width:190px;cursor:pointer;height:34px;border:1px solid #d0d0e7;line-height: 34px;border-radius: 3px;padding:0 5px;background:url(../../images/gp/icon.png) no-repeat #fff;background-position: 100% -1902px;}

.methods .func{line-height:36px;margin-top:22px;float: left;  font-size:13px;height:36px;}
.methods .func .default{height:36px;float:left;color:#343b66;margin-left:15px}
.methods .func .query_func{width:85px;height:36px;background:#ff6600;color:#fff;display:block;text-align: center;border-radius: 3px;float: left}

.overview_data{width: 96%; color: #5d6383; overflow: hidden; margin:29px auto;}
.overview_data .data_box{width:22%;float: left;  background: #f5f5fb;overflow: hidden;margin-right:4%;  -webkit-box-shadow: 0 0 0 1px #d8d8e8 inset;  -moz-box-shadow: 0 0 0 1px #d8d8e8 inset;  box-shadow: 0 0 0 1px #d8d8e8 inset;}
.overview_data .data_box p{width:88%;float: left;white-space:nowrap;  overflow:hidden;  text-overflow:ellipsis;margin-left:6%;padding-top:3%;font-size: 14px;color: #5d6383}
.overview_data .data_box h1{color:#ff6868;float: left;padding:1% 0 1% 20px}
.overview_data .data_box p span{color:#ff6868;margin-left:17px}

