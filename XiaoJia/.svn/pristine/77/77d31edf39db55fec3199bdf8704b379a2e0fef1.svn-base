<script type="text/javascript">

    //招生范围
    $(".fill-in-box .fill-data .fill-radio a").click(function () {
        $(".fill-in-box .fill-data .fill-radio a").removeClass("cur2");
        $(this).addClass("cur2")
    });
    //标签
    $(".fill-label span i").click(function () {
        $(this).parent().remove();
        if(!$(".fill-label span i").length){
            $(".fill-in-box .fill-data .fill-label").css("marginBottom","0px")
        }
    });

</script>

