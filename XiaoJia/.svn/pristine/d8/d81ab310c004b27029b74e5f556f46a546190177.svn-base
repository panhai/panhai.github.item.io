    });
    //下架点击事件
    $(".obtained").click(function () {
        layer.open({
            title:'提示',
            area: '576px',
            type:1,
            skin: '',
            content: $(".togetherLayer"),
        });
    });
    $(".togt").click(function () {
        if ($(this).find("i").hasClass('cur')) {
            $(this).find("i").removeClass('cur');
        } else {
            $(this).find("i").addClass('cur')
        }
    });


    //二维码显示隐藏事件
    $(".attributes-menu .attr-message p b").mouseenter(function () {
        $(".qr-layer").fadeIn(200);
    }).mouseleave(function () {
        $(".qr-layer").fadeOut(0);
    });
    $(".plus-and-select .table-piece table tr td:last-child a").click(function () {
        if ($(this).hasClass('cur')) {
            $(this).removeClass('cur');
            $(this).text('选择');
        } else {
