<script type="text/javascript">

    //确认到账浮层   //与财务管理JS有所不同


    $(".confirm-receipt").click(function () {
        var self = this;
        confirmReceipt(self)
    });
    function confirmReceipt(ele){
        var getHtml = $(ele).parent().html();
        var parent = $(ele).parent();
        $(ele).parent().children().remove();
        parent/*删除append*/.queue(function () {
            $(this).children().click(function () {
                layer.open({
                    title:"详情",
                    type:"1",
                    area:"682px",
                    content:$("#details-layer-one")
                })
            });
            $(this).dequeue()
        });

        $(".sure-layer").addClass("cur");

        $(".cancel-sure").click(function () {
            parent.html(getHtml).queue(function (ele) {
                $(this).children(".confirm-receipt").click(function () {
                    var elem = this;
                    confirmReceipt(elem)
                });

                $(this).children(".cancel-key").click(function () {
                    layer.open({
                        title:"详情",
                        type:"1",
                        area:"682px",
                        content:$("#details-layer-two")
                    })
                });
                $(this).dequeue()
            });
            clearInterval(inter);
            $(".last-time").text("（5）");
            $(".sure-layer").removeClass("cur");
        });

        var i = 0;
        var t;
        var inter = setInterval(function () {
            ++i;
            t = 5 - i;
            $(".last-time").text("（"+t+"）");
            if (t<=0){
                clearInterval(inter);
                $(".sure-layer").removeClass("cur");
                $(".last-time").text("（5）");
            }
        },1000);
        $(".sure-layer i").click(function () {
            clearInterval(inter);
            $(".sure-layer").removeClass("cur");
            $(".last-time").text("（5）");
        })
    }




    //解除原因
    $(".unbind").click(function () {
        layer.open({
            title:'解除原因',
            area: '600px',
            type:1,
            skin: '',
            content: $("#why"),
        });
    });



    //推广课程
    $(".promotion-course").click(function () {
        layer.open({
            title:'推广课程',
            area: '600px',
            type:1,
            skin: '',
            content: $("#promotion-course-layer"),
        });
    });
