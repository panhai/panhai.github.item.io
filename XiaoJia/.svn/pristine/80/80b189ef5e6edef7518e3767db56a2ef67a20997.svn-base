    });

    $(".plus-sth").click(function () {
        layer.open({
            title: '教材杂费',
            type: 1,
            area: '488px',
            skin: '',
            content: $('#plus-sth')
        });
    });

    $(".fill-radio>a.cur1").click(function () {
        layer.open({
            title: '新增班级',
            type: 1,
            area: '804px',
            skin: '',
            content: $('#plus-and-select-class')
        });
    });


    //性别
    $(".fill-in-box .fill-radio a").click(function () {
        $(".fill-in-box .fill-radio a").removeClass("cur");
        $(this).addClass("cur")
    });
    
    //发现同学手机
    $(".find-telephone").click(function () {
        layer.open({
            title: '发现同手机学员',
            type: 1,
            area: '670px',
            skin: '',
            content: $('#find-phonenumber')
        });
    });

    //批量导入
    $(".import").click(function () {
        $(this).publicPrompt({
            title:'如何批量导入报名学员？',
            message:"1、下载Excel模板，按模板要求录入学员信息并保存；",
            message1:"2、点击“导入Excel表”，选择刚保存的Excel表格确定即可导入",
            btn1:"下载Excel模板",
            btn2:"导入Excel表",
            func1:function () {

            },
            func2:function () {

            }
        });
        $(".togetherLayer .toTitle img").remove();
        $(".togetherLayer .toTitle").css("marginLeft","42px");
        $(".submitBtn a").addClass("funcSe");
    });





    $(".open-key").click(function () {
        if ( $(this).siblings("i").hasClass('cur')) {
            $(".specific-information,.hidden-floor").hide();
            $(this).siblings("i").removeClass("cur");
        } else {
            $(".specific-information,.hidden-floor").show();
            $(this).siblings("i").addClass("cur");
        }
    });

    $(".key-for").click(function () {
        if ($(this).hasClass('cur')) {
            $(this).removeClass('cur');
            $(".hidden-layer").hide();
            $(this).find("i").text("+");
        } else {
            $(this).addClass('cur');
            $(".hidden-layer").show();
            $(this).find("i").text("-");
        }
    });



    VitaeSource();
