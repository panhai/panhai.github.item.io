// 公共样式
@color3: #333;
@color6: #666;
@color9: #999;
@color11: #03bf84;
@color12: #547ed1;
@color13: #066cbd;
@btn_bgcolor: #03bf84;
.clearfix {
    clear: both;
    zoom: 1;
}
.colorg3{
    color: #03bf84 !important;
}
button{
    border: none;
    cursor: pointer;
    outline: none;
    background-color: #fff;
}
textarea,input,button{
    outline: none;
}
textarea{
    overflow: hidden;
}
.clearfix:after,
.clearfix:before {
    content: ".";
    display: block;
    clear: both;
    visibility: hidden;
    line-height: 0;
    height: 0;
}
.nopadding{padding:0!important}
.nomargin{margin:0!important}
.nomargin-bottom{margin-bottom:0!important}
.noborder{border:none!important}
.nobg{background:none!important}
.noborder-top{border-top:none!important}
.noborder-bottom{border-bottom:none!important;padding: 2px 10px}
.border-left{border-left:none!important}
.border-right{border-right:none!important}
.clearfix{zoom:1}
.white_bg{background:#fff}
input{background:0 0}

.SELECT{border:1px solid #d8d8d8;border-radius:3px;width:230px;height:31px;font-size:14px;color:#2a2a2a;display:inline-block;vertical-align:middle;margin-left:-5px}
.SELECT dt{
    line-height:31px;clear:both;width:230px;position:relative;text-align:left;
    color:#2a2a2a;font-size:14px;cursor:pointer;text-indent:10px;
    background: url(../../images/dt.png) no-repeat;
    background-position: 208px 50%;
    opacity: 0.4;
}
.SELECT dt span{font-size:14px;color:#2a2a2a;vertical-align:middle}
.SELECT dt b{font-size:14px;color:#2a2a2a;display:inline-block;vertical-align:middle;text-indent:0}
.SELECT dt b{
    width: 90%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    vertical-align: top;
}
.SELECT dt i{display:inline-block;vertical-align:middle;height:12px;width:15px;margin-left:8px}
.SELECT dd{display:none;position:absolute;width:230px;background:#fff;z-index:2;left:0}
.SELECT dd.normal{
    /* border: solid 1px #e2e7eb; */
}
.SELECT dd a{line-height:30px}
.SELECT-on{position:relative}
.SELECT-on dt{z-index:2}
.SELECT-on dd{display:block;z-index:1000;top:20px;left:85px}
.SELECT-on dd a{display:block;line-height:30px;color:#2a2a2a;font-size:14px;text-indent:12px;cursor:pointer}
.SELECT-on dd a:hover{background:#e2efff;color:#0068cd}
.SELECT-on dd a.cur{background:#e2efff;color:#0068cd}
.SELECT-on dt i{background-position:-87px -21px}
.SELECT-on .normal{width:100%;border:1px solid #d8d8d8;top:33px;left:0}
.SELECT-on .normal.maxHeight{max-height:300px;overflow:auto}

.SELECT-on .normal.CITY{border:0}
.SELECT-on .normal.JOBPOSITIONS{border:0}
.SELECT.Dept dt b{display:inline-block;max-width:90%;overflow:hidden;text-overflow:ellipsis;white-space:nowrap}
.City_list .region_type{padding-top:18px;border-bottom:1px solid #d8d8d8;height:40px;line-height:40px}
.City_list .region_type span{display:block;float:left;width:70px;height:39px;border-bottom:2px solid none;color:#414141;font-size:16px;text-align:center;cursor:pointer}
.City_list .region_type span.cur{border-bottom:2px solid #0068cd;color:#0068cd}
.City_list .type_list{width:380px;padding:20px 0}
.City_list .type_list .letter_list{display:none}
.City_list .type_list .letter_list .groupLine{margin-bottom:10px}
.City_list .type_list .letter_list .groupLine h4{display:block;float:left;font-size:16px;color:#2d3c4b;font-weight:700;width:50px;text-align:center}
.City_list .type_list .letter_list .groupLine p{float:left;width:300px}
.City_list .type_list .letter_list .groupLine p a{display:inline-block;padding:0 0 10px 20px;color:#2d3c4b;font-size:14px;line-height:15px;cursor:pointer}
.City_list .type_list .letter_list .groupLine p a:hover{color:#0068cd;background:0 0}
.City_list .type_list .option_list{display:none}
.City_list .type_list .option_list a{display:inline-block;padding:0 0 10px 15px;color:#2d3c4b;font-size:14px;line-height:15px;cursor:pointer}
.City_list .type_list .option_list a:hover{color:#0068cd;background:0 0}
.City_list .type_list .show{display:block}
.fill-in-box .City_list {
    position: absolute;
    width: 380px;
    border: 1px solid #d8d8d8;
    border-radius: 2px;
    z-index: 2;
    background: #fff;
    padding: 0 25px;
    left: 0;
    top: -2px;
    z-index: 1500;
}
.field-validation-error {
    margin: 5px 0 5px 86px;
    color:#fb6f12;
    font-size: 14px;
    display: block;
    line-height: 20px;
    padding-left: 20px;
    background: url(../../images/icon.png) no-repeat;
    background-position: -4px -211px;
    background: url(../../images/warning-1.png) no-repeat left 50%;
    -webkit-background-size: auto 80%;
    background-size: auto 80%;
}
.head-navgater .search{float:right;border-radius:20px;line-height:26px;height:26px;border:1px solid #d4d4d4;width:266px;position:relative;margin-top:8px;}
.head-navgater  .search .search_txet{display:inline-block;border:0;background:#fff;font-size:12px;color:#393939;font-family:"微软雅黑";line-height:24px;height:24px;width:220px;margin-left:12px;float: left;}
.head-navgater  .search .icon{display:block;position:absolute;right:10px;top:5px;cursor:pointer;width:16px;height:16px;overflow:hidden;background:url(../../images/icon.png) no-repeat;background-position: 0px -39px;}
.head-navgater  .search .history-layer{
    width: 268px;
    height: auto;
    position: absolute;
    top: 27px;
    left: 0px;
    background: #fff;
    z-index: 10;
    -webkit-box-shadow: 1px 1px 8px 3px rgba(238,238,238,.9);
    -moz-box-shadow:    1px 1px 8px 3px rgba(238,238,238,.9);
    box-shadow:         1px 1px 8px 3px rgba(238,238,238,.9);
    overflow: auto;
    padding:5px 0;
    display: none;
}
.head-navgater  .search .history-layer a{
    height: 32px;
    line-height: 32px;
    display: block;
    font-size: 12px;
    padding:0 10px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.head-navgater  .search .history-layer a:hover{
    background: #f4faff;
}

.head-navgater  .search .history-layer a i{
    display: inline-block;
    padding-left: 18px;
    line-height: 15px;
    height: 15px;
    background: url(../../images/icon.png) no-repeat;
    background-position: 0px -39px;
    color: #666666;
}
.head-navgater  .search .history-layer a span{
    color: #fb6f12;
}

body {
    background-color: #f2f5f8;
}

// 头部搜索
.head_navgater {
    height: 44px;
    width: 100%;
    float: right;
    .clearfix;
    background-color: #fff;
    position: relative;
    .rightCenter {
        width: 600px;
        float: right;
        height: 100%;
        .search {
            padding-right: 25px;
            .num {
                position: relative;
                width: 24px;
                margin: 0 11px 0 23px;
            }
            .num b {
                position: absolute;
                top: 5px;
                right: 4px;
                width: 14px;
                height: 14px;
                border-radius: 50%;
                background-color: #fa6d0f;
                color: #fff;
                text-align: center;
                line-height: 14px;
                font-size: 10px;
                display: inline-block;
            }
            .num .icon {
                display: inline-block;
                width: 13px;
                height: 16px;
                overflow: hidden;
                background: url(../../images/icon.png) no-repeat;
                background-position: -1px -76px;
                cursor: pointer;
                vertical-align: middle;
            }
            .from input {
                display: inline-block;
                width: 100%;
                height: 100%;
                background-color: #fbfbfb;
                border-radius: 13px;
                border: solid 1px #eeeeee;
                opacity: 0.8;
                padding-left: 12px;
                padding-right: 50px;
                box-sizing: border-box;
            }
            .from input:focus {
                border-color: #066cbd;
            }
            .from {
                position: relative;
                width: 266px;
                height: 26px;
                display: inline-block;
            }
            .from i {
                display: block;
                position: absolute;
                right: 14px;
                top: 14px;
                cursor: pointer;
                width: 16px;
                height: 16px;
                overflow: hidden;
                background: url(../../images/icon.png) no-repeat;
                background-position: 0px -39px;
                z-index: 4;
            }
        }
        .search>div {
            float: right;
            height: 44px;
            line-height: 44px;
            a {
                color: @color3;
                font-size: 12px;
                margin-left: 12px;
            }
            a:hover {
                color: #066cbd;
            }
        }
    }
}

// 左侧固定导航
.left_navgater {
    position: fixed;
    width: 160px;
    height: 100%;
    background-color: #3e4d66;
    z-index: 50;
    .logo {
        width: 100%;
        height: 44px;
        line-height: 44px;
        position: relative;
        margin: 10px 0;
        background: url(../../images/login_logo.png) no-repeat 14px 50%;
        background-size: 43px 38px;
        span {
            color: #feffff;
            margin-left: 62px;
            position: relative;
            top: 3px;
            opacity: 0.6;
        }
        .changeArrow {
            display: inline-block;
            float: right;
            width: 12px;
            height: 25px;
            line-height: 25px;
            background-color: rgba(255, 255, 255, 0.6);
            border-radius: 4px 0 0 4px;
            margin-top: 9px;
            text-align: center;
            i {
                display: inline-block;
                vertical-align: middle;
                border: 3px solid transparent;
                border-right-color: #fff;
                margin-top: -2px;
            }
        }
    }
    .info {
        width: 100%;
        display: table;
        padding-bottom: 16px;
        border-bottom: 1px solid #293447;
        -webkit-box-shadow: 0px -1px 0px 0px rgba(255, 255, 255, .1) inset;
        -moz-box-shadow: 0px -1px 0px 0px rgba(255, 255, 255, .1) inset;
        box-shadow: 0px -1px 0px 0px rgba(255, 255, 255, .1) inset;
        font-size: 0px;
        .clearfix;
        .person {
            float: left;
            margin-left: 22px;
        }
        .person img {
            display: block;
            width: 46px;
            height: 46px;
            border-radius: 50%;
            vertical-align: middle;
        }
        .user {
            float: left;
            font-size: 14px;
            color: #fff;
            padding-left: 2px;
            padding-top: 4px;
            .kf {
                color: #fff;
                opacity: 0.7;
                margin-top: 10px;
            }
            .tearch {}
        }
    }
    .nav {
        width: 100%;
        a {
            display: block;
            height: 34px;
            line-height: 34px;
            text-align: left;
            margin-top: 6px;
            border-left: 4px solid transparent;
            span {
                color: #fff;
                vertical-align: middle;
            }
            i {
                display: inline-block;
                position: relative;
                width: 14px;
                height: 14px;
                background: url(../../images/daohtub-1.png) no-repeat;
                vertical-align: middle;
                margin-left: 20px;
                margin-right: 12px;
            }
            i.position1 {
                background-position: 0 -1px;
            }
            i.position2 {
                background-position: -72px 0px;
            }
            i.position3 {
                background-position: -23px 0px;
            }
            i.position4 {
                background-position: -98px 0px;
            }
            i.position5 {
                background-position: -173px 0px;
            }
            i.position6 {
                background-position: -47px -1px;
            }
            i.position7 {
                background-position: -122px 0px;
            }
            i.position8 {
                background-position: -197px 0px;
            }
            i.position9 {
                background-position: -148px 0px;
            }
        }
        a.cur {
            background-color: #243348;
            border-left-color: #03bf84;
            i.position1 {
                background-position: 0 -22px;
            }
            i.position2 {
                background-position: -72px -21px
            }
            i.position3 {
                background-position: -23px -21px;
            }
            i.position4 {
                background-position: -98px -22px;
            }
            i.position5 {
                background-position: -173px -22px;
            }
            i.position6 {
                background-position: -47px -24px;
            }
            i.position7 {
                background-position: -122px -23px;
            }
            i.position8 {
                background-position: -197px -23px;
            }
            i.position9 {
                background-position: -148px -23px;
            }
        }
    }
}

// 小导航
.left_small_navgater {
    position: fixed;
    width: 54px;
    height: 100%;
    background-color: #3e4d66;
    z-index: 50;
    .logoBox {
        width: 100%;
        height: 44px;
        position: relative;
        margin: 10px 0;
        background: url(../../images/login_logo.png) no-repeat 50% 50%;
        background-size: 36px 36px;
        position: relative;
        .changeArrow {
            display: inline-block;
            float: right;
            width: 12px;
            height: 25px;
            margin-right: -12px;
            line-height: 25px;
            background-color: rgba(6, 108, 189, .5);
            border-radius: 4px 0 0 4px;
            margin-top: 9px;
            text-align: center;
            transform: rotateZ(180deg);
            i {
                display: inline-block;
                vertical-align: middle;
                border: 3px solid transparent;
                border-right-color: #fff;
                margin-top: -2px;
            }
        }
    }
    .school_infor {
        width: 100%;
        overflow: hidden;
        padding-bottom: 16px;
        border-bottom: 1px solid #293447;
        -webkit-box-shadow: 0 -1px 0 0 rgba(255, 255, 255, .1) inset;
        -moz-box-shadow: 0 -1px 0 0 rgba(255, 255, 255, .1) inset;
        box-shadow: 0 -1px 0 0 rgba(255, 255, 255, .1) inset;
        text-align: center;
        img {
            width: 40px;
            height: 40px;
            background-color: #fff;
            -webkit-border-radius: 50%;
            -moz-border-radius: 50%;
            border-radius: 50%;
        }
    }
    .small_nav {
        .nav_url.cur {
            background-color: #fb6f12;
            border-left: 3px solid #03bf84;
        }
        .nav_url {
            height: 34px;
            margin-top: 6px;
            position: relative;
            cursor: pointer;
            z-index: 10;
            text-align: center;
            background-color: #3e4d66;
            .url_icon {
                display: inline-block;
                margin: 10px auto 0;
                width: 14px;
                height: 14px;
                background: url(../../images/daohtub-1.png) no-repeat 50% 50%;
                border-left: 3px solid transparent;
                box-sizing: border-box;
            }
            .position1 {
                background-position: -3px -1px;
            }
            .position2 {
                background-position: -75px 0;
            }
            .position3 {
                background-position: -151px 0px;
            }
            .position4 {
                background-position: -26px 0px;
            }
            .position5 {
                background-position: -101px 0px;
            }
            .position6 {
                background-position: -176px 0px;
            }
            .position7 {
                background-position: -50px -1px;
            }
            .position8 {
                background-position: -125px 0px;
            }
            .position9 {
                background-position: -200px 0px;
            }
            &:hover {
                background-color: #fb6f12;
                border-left: 3px solid #03bf84;
            }
            &:hover .position1 {
                background-position: -3px -22px;
            }
            &:hover .position2 {
                background-position: -75px -22px;
            }
            &:hover .position3 {
                background-position: -151px -21px;
            }
            &:hover .position4 {
                background-position: -26px -21px;
            }
            &:hover .position5 {
                background-position: -101px -21px;
            }
            &:hover .position6 {
                background-position: -176px -21px;
            }
            &:hover .position7 {
                background-position: -50px -21px;
            }
            &:hover .position8 {
                background-position: -125px -21px;
            }
            &:hover .position9 {
                background-position: -200px -23px;
            }
            .moveLayer {
                min-height: 34px;
                position: absolute;
                top: 0;
                left: 51px;
                width: 83px;
                z-index: 1;
                background-color: #7aafd9;
                z-index: -10;
                display: none;
                -webkit-transform: translateZ(0) translateX(0);
                -moz-transform: translateZ(0) translateX(0);
                -ms-transform: translateZ(0) translateX(0);
                -o-transform: translateZ(0) translateX(0);
                transform: translateZ(0) translateX(0);
                i {
                    position: absolute;
                    border: 5px solid transparent;
                    border-right-color: #7aafd9;
                    top: 12px;
                    left: -10px;
                }
                a {
                    display: block;
                    height: 34px;
                    line-height: 34px;
                    text-align: center;
                    color: #fff;
                    &:hover {
                        color: #066cbd;
                    }
                }
            }
        }
    }
}

