            skin: 'UPCompanyLogo',
            content: $('#UPCompanyLogo')
        });
    })



    $(".plus-and-select .table-piece table tr td:last-child a").click(function () {
        if ($(this).hasClass('cur')) {
            $(this).removeClass('cur');
            $(this).text('选择');
        } else {
            $(this).addClass('cur');
            $(this).text('已选');
        }
    });
    $(".fill-in-box .promotion-layer .pro-url").click(function () {
        if($(this).find("i").hasClass("cur")){
            $(this).find("i").removeClass("cur");
        }else {
            $(this).find("i").addClass("cur");
        }
    })


    $(".plus-and-select-layer").click(function () {
        layer.open({
            title: '添加账号',
            type: 1,
            area: ' 738px',
            skin: '',
            content: $('#plus-and-select')
        });
    });
    $(".click-window").click(function () {
        $(".fill-types").show();
    });
    $(".fill-types .right-select a").click(function () {
        $(".fill-types").hide();
    });
    $(".this-fill").mouseleave(function () {
        $(".fill-types").hide();
    });

    // 百度地图API功能
    var map = new BMap.Map("mapForBox");    // 创建Map实例
    var point = new BMap.Point(113.374722,23.131981);
    map.centerAndZoom(new BMap.Point(113.374722,23.131981), 15);  // 初始化地图,设置中心点坐标和地图级别
    //添加地图类型控件
    map.addControl(new BMap.MapTypeControl({
        mapTypes:[
            BMAP_NORMAL_MAP,
            BMAP_HYBRID_MAP
        ]}));
    map.setCurrentCity("广州");          // 设置地图显示的城市 此项是必须设置的
    map.enableScrollWheelZoom(true);     //开启鼠标滚轮缩放

    var marker = new BMap.Marker(point);        // 创建标注
    map.addOverlay(marker);

    var opts = {
          // 信息窗口宽度
        title : "广州市天河区棠下街道建业路华翠街66号202G" // 信息窗口标题
    }
    var infoWindow = new BMap.InfoWindow("", opts);  // 创建信息窗口对象
    map.openInfoWindow(infoWindow, map.getCenter());
    marker.addEventListener("click", function(){
        map.openInfoWindow(infoWindow,point); //开启信息窗口
    });

</script>

