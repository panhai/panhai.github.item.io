<script type="text/javascript">
    $(".fill-in-box .fill-data .fill-radio a").click(function () {
        $(".fill-in-box .fill-data .fill-radio a").removeClass("cur2");
        $(this).addClass("cur2")
    });

    $(".setting-other").click(function () {
        layer.open({
            title: '上传Logo',
            type: 1,
            area: '615px',
            skin: 'UPCompanyLogo',
            content: $('#UPCompanyLogo')
        });
    })


    //类别弹框点击事件
    $(".jobs").click(function () {
        $(".theme-big-selectbox").show()
    });
    $(".theme-sele").mouseleave(function () {
        $(".theme-big-selectbox").hide()

    });
    //职位选择
    $(".job-data a").click(function () {
        if ($(this).hasClass('cur')) {
            $(this).removeClass('cur');
        } else {
            $(this).addClass('cur')
        }
    })



</script>

