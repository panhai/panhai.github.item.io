<script src="../js/layer.js"></script>
<script src="../js/area.js"></script>
<script src="../js/all.js"></script>
<script src="../js/plugin/select.js"></script>
<script src="../js/My97DatePicker/WdatePicker.js"></script>
<script src="http://api.map.baidu.com/api?v=2.0&amp;ak=OlGzoCwcKN9OvM16T9RnFwOl"></script>

<script type="text/javascript">


    $(".set-write").click(function () {
        layer.open({
            title: '修改回访计划',
            type: 1,
            area: '744px',
            skin: '',
            content: $('#set-write')
        });
    });






    //跟进记录学员状态点击事件
    $(".fill-in-box .fill-radio a").click(function () {
        $(".fill-in-box .fill-radio a").removeClass("cur");
        $(this).addClass("cur")
    });
    var getLeft = $(".right-for-content").offset().left;
    //head  右侧模板
    $(".head .right-head").css("left",getLeft+"px");

    //导航条滚动计算
    $(window).scroll(function (e) {
        e.stopPropagation();
        var getScrollTop = $(window).scrollTop();
        var getScrollLeft = $(window).scrollLeft();
        if( getScrollTop>=70) {
            $(".right-for-content").css("position","fixed").css("left",getLeft - getScrollLeft+"px").css("top","0")
        }else {
            $(".right-for-content").css("position","static")
        }

    });



    //发通知
    $(".other-cur2").click(function () {
        layer.open({
            title: '发通知',
            type: 1,
            area: '744px',
            skin: '',
            content: $('#send-msg')
        });
    });


    //下次回访
    $(".shortcut-selec").click(function () {
        $(".fill-window").show();
    });
    $(".schedule-layer").click(function () {
        layer.open({
            title: '添加跟进记录',
            type: 1,
            area: '790px',
            skin: '',
            content: $('#comment-window')
        });
    });

    //勾选事件
    $(".fill-in-box .promotion-layer .pro-url").click(function () {
        if($(this).find("i").hasClass("cur")){
            $(".fill-in-box .promotion-layer .pro-url").find("i").removeClass("cur");
            $(this).find("i").removeClass("cur");
        }else {
            $(".fill-in-box .promotion-layer .pro-url").find("i").removeClass("cur");
            $(this).find("i").addClass("cur");
        }
    });








    //+标签事件
    $(".open-key").click(function () {
        var getLeft = $(this).position().left;

        $(".label-layer").show().css("left",getLeft+"px").css("top","75px");
    });
    $(".esc").click(function () {
        $(".label-layer").hide();
    });
    $(".label-layer").mouseleave(function () {
        $(".label-layer").hide();
    });
    $(" .label-name a").click(function () {
        if ($(this).hasClass('cur')) {
            $(this).removeClass('cur');
        } else {
            $(this).addClass('cur');

        }
    });

    //只看自己
    $(".see-myself").click(function () {
        if ($(this).find("i").hasClass('cur')) {
            $(this).find("i").removeClass('cur');
        } else {
            $(this).find("i").addClass('cur');

        }
    });




</script>

</body>
</html>