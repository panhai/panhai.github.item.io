    $(".setting-other").click(function () {
        layer.open({
            title: '上传企业Logo',
            type: 1,
            area: '615px',
            skin: 'UPCompanyLogo',
            content: $('#UPCompanyLogo')
        });
    })


    //导航多选事件
    $(".option").click(function () {
        if($(this).hasClass("cur")){
            $(this).removeClass("cur");
        }else {
            $(this).addClass("cur");
        }
    });

    //导航切换样式事件

    $(".left-navgater .logoBox .changeArrow").click(function () {
        $(".left-navgater").hide();
        $(".left-small-navgater").show();
        $(".right-content").addClass("dif")

    })

