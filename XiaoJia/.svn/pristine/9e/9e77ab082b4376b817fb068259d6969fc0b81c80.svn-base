        $(".left-navgater").show();
        $(".left-small-navgater").hide();
        $(".right-content").removeClass("dif")
    })


    //小导航hover事件
    $(".left-small-navgater .small-nav .nav-url").on("mouseenter",function () {

        $(this).addClass("cur");
        $(this).find(".moveLayer").fadeIn(0).addClass("moveClass")

    }).on("mouseleave",function () {

        $(this).removeClass("cur");
        $(this).find(".moveLayer").fadeOut(0).removeClass("moveClass")

    });
    $(".moveLayer").mouseover(function (e) {
        e.stopPropagation();
        e.preventDefault();
    });


    //编辑切换JS
    $(".fill-in-box .set").click(function () {
        $(".see-layer").hide();
        $(".set-layer").show();
    });
    $(".fill-in-box .save").click(function () {
        $(".see-layer").show();
        $(".set-layer").hide();
    });


    $('.right-content .tips-text').hover(function () {
        layer.tips('<div style="width:182px;padding:4px;color:#333">Logo将在推广贵校课程时用到，故需提交\n' +
            '校+运营审核，审核通过后，您即可使用此Logo\n' +
            '用于推广！\n</div>', $(this), {
            tips: [1, 'rgba(248,248,252,0.9)'],
            time: 0
        });
    }, function () {
        layer.closeAll('tips');
    });

</script>
</body>
</html>