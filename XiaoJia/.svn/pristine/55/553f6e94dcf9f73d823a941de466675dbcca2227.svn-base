<script type="text/javascript">

    $(".plus-label-key").click(function (event) {
        event.stopPropagation();
        $(document).keydown(function(event){
            if(event.keyCode == 13){
                var value = $(".plus-label-key").val();
                $(".plus-label-key").val("");
                if(value==""){
                    return
                }else {
                    $(".fill-label").append("<span>"+value+"<i>+</i></span>")
                }


            }
        });
    })




    $(".fill-label span i").click(function () {
