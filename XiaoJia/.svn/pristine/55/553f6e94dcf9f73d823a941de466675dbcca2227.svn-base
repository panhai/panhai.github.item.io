//图片选择事件 //最大6张选择事件
    var macthingImg = 0;
    $(".macthing-img").click(function () {

        if($(this).hasClass("judgment")){
            --macthingImg;
            $(this).removeClass("judgment");
            $(this).find(".macthing-bg").hide();
            $(this).find(".macthing-tick").hide();
            $(this).find(".macthing-close").show();
            $(".em1").text(macthingImg);
            $(".em2").text(6 - macthingImg)
        }else {
            if(macthingImg>=6) {
                $(".t_foot .leftbox label").addClass("warning").delay(1000).queue(function () {
                    $(this).removeClass("warning");
                    $(this).dequeue()
                });
                return
            }
            ++macthingImg;
            $(this).addClass("judgment");
            $(this).find(".macthing-bg").show();
            $(this).find(".macthing-tick").show();
            $(this).find(".macthing-close").hide();
            $(".em1").text(macthingImg);
            $(".em2").text(6 - macthingImg);

        }
    });
    $(".macthing-img").mouseenter(function () {
        if($(this).hasClass("judgment")){
            $(this).find(".macthing-close").hide();
        }else {
            $(this).find(".macthing-close").show();
        }
    });
    $(".macthing-img").mouseleave(function () {
        $(this).find(".macthing-close").hide();
    });
    $(".macthing-close").click(function () {
        $(this).parent().remove()
    });



