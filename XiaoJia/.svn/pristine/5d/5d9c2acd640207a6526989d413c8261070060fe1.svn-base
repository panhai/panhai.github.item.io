.fill-in-box input[type="radio"] + label::before {
    content: "\a0"; /*不换行空格*/
    display: inline-block;
    vertical-align: middle;
    font-size: 18px;
    width: 18px;
    height: 18px;
    border-radius: 50%;
    background-color: #f3f6f8;
    border:1px solid #e3e3e3;
    text-indent: 10px;
    line-height: 1;
    cursor: pointer;
    margin-right:5px;
}

.fill-in-box .midlleBoxs{
    display: inline-block;
    margin-right:20px;
}


.fill-in-box  input[type="radio"]:checked + label::before {
    height: 8px;
    width: 8px;
    background-color: #797b7d;
    background-clip: content-box;
    padding: 5px;
}
.fill-in-box  input[type="radio"]:checked + label:after {
    height: 8px;
    width: 8px;
    background-color: #898989;
    background-clip: content-box;
    padding: 5px;
}

.fill-in-box input[type="radio"] {
    position: absolute;
    clip: rect(0, 0, 0, 0);
    display: none;
}
