}
.associationLayer{
    display: none;
}
.submitOk{
    width: 74px;
    height: 32px;
    line-height: 32px;
    display: inline-block;
    text-align: center;
    color: #ffffff;
    font-size: 14px;
    position: absolute;
    background: #0068cd;
    right: 15px;
    bottom: 18px;
    outline: none;
    border:none
}
.submitOk:hover{
    color: #ffffff;
}

.head{
    height: 50px;
    line-height: 52px;
    background-color: #ffffff;
    box-shadow: 0px 1px 2px 0px  rgba(195, 195, 195, 0.46);
    position: relative;
}
.head .left-head{
    display: inline-block;
}
.head .left-head img{
    height: 24px;
    vertical-align: middle;
    display: inline-block;
    position: relative;
    top: -2px;
    margin-left: 20px;
}
.head .left-head .menu{
    display: inline-block;
    margin-left: 60px;
    line-height: 50px;
}
.head .left-head .menu .name{
    font-size: 14px;
    color: #333333;
    padding-right: 10px;
    font-weight: bold;
}
.head .left-head .menu .job{
    font-size: 12px;
    padding-left: 10px;
    color: #999999;
    border-left:1px solid #dee4e8;
    line-height: 16px;
    display: inline-block;
    max-width:370px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    height: 16px;
    vertical-align: middle;
    position: relative;
    top: -2px;
}
.head .left-head .menu .bg1{
    font-size: 12px;
    background-color: #066cbd;
    border-radius: 2px;
    padding:3px 8px;
    color: #ffffff;
}
.head .left-head .menu .bg2{
    font-size: 12px;
    background-color: #fa6d0f;
    border-radius: 2px;
    padding:3px 8px;
    color: #ffffff;
}
.head .right-head{
    height: 50px;
    line-height: 50px;
    position: absolute;
    top: 0;
}
.head .right-head span{
    font-size: 12px;
    color: #999999;
}
.head .right-head i{
    font-size: 12px;
    color: #999999;
    display: inline-block;
    width: 16px;
    height: 16px;
    background: url(../images/write-2.png) no-repeat 50% 50%;
    vertical-align: middle;
    position: relative;
    top: -1px;
    margin-left: 4px;
    cursor: pointer;
}



