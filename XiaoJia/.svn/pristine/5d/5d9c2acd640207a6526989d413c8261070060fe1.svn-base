.fixedBg{
    position: fixed;
    top: 0;
    left: 0;
    background: rgba(0,0,0,.5);
    width: 100%;
    height: 100%;
    display: none;
    z-index: 1000;
}
.fixedBg .imgBox{
    display: table-cell;
    vertical-align: middle;
    text-align: center;
}
#photoLayer{
    display: inline-block;
    width: 80%;
    height: auto;
    margin: auto;
    position: relative;
}
.fixedBg .imgBox img{
    display: block;
    height: auto;
    max-width: 80%;
    margin: auto;
}
.fixedBg .imgBox .previous{
    width: 80px;
    height: 80px;
    background: rgba(0,0,0,0.3);
    position: absolute;
    top: calc(50% - 40px);
    left: 0;
    cursor: pointer;
    -webkit-transition: ease .2s;
    -moz-transition: ease .2s;
    -ms-transition: ease .2s;
    -o-transition: ease .2s;
    transition: ease .2s;
}
.fixedBg .imgBox .previous:hover{
    background: rgba(0,0,0,0.5);
}
.fixedBg .imgBox .next{
    width: 80px;
    height: 80px;
    position: absolute;
    top: calc(50% - 40px);
    right: 0;
    cursor: pointer;
    background: rgba(0,0,0,0.3);
    -webkit-transition: ease .2s;
    -moz-transition: ease .2s;
    -ms-transition: ease .2s;
    -o-transition: ease .2s;
    transition: ease .2s;
}
.fixedBg .imgBox .next:hover{
    background: rgba(0,0,0,0.5);
}
.fixedBg .imgBox .previous img,.fixedBg .imgBox .next img{
    width: 80px;
    height: 80px;
}
.fixedBg .close{
    position: absolute;
    top: 20%;
    right:10%;
    font-size: 40px;
    color: #ffffff;
    border:1px solid #Fff;
    width: 40px;
    height: 40px;
    font-family: "微软雅黑 Light";
    line-height: 31px;
    font-weight: normal;
    text-align: center;
    border-radius:50%;
    cursor: pointer;
    z-index: 100;
}

