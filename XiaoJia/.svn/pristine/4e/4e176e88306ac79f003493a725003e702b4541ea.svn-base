			//控制文本两行显示 ...  diffrent-co
			function Trim(str,is_global){
	            var result;
	            result = str.replace(/(^\s+)|(\s+$)/g,"");
	            if(is_global.toLowerCase()=="g"){
	                result = result.replace(/\s/g,"");
	            }
	
	            return result;
	        }

			function font_show(demoClass) {
				var tfont = Trim($(demoClass).text(),'g');
				var font_length = tfont.length;
				
				if(font_length > 90){
					$('.props').css({position:'relative'})
					var cont = tfont.slice(0,font_length)
					console.log(cont,font_length)
					$(demoClass).text(cont)
					var $cfonttext = $('<p style="">...</p>')
					$cfonttext.css({background:'#fff',width:'20px','text-indent':'0px','padding-right':'1px','opacity':'1',position:'absolute',color:'#000',top:'15px',right:'9px',fontSize:'16px','text-align':'center'})
					$('.props').append($cfonttext)
				}
			}
			
			
			$(function(){
				font_show('.props .diffrent-co')
			})
			
