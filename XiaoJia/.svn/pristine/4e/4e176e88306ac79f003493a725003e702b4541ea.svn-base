		<script type="text/javascript">
			//类别弹框点击事件
			$(".jobs").click(function(e) {
				e.stopPropagation();
				$(".theme-big-selectbox").show()
			});
			$(document).click(function() {
				$(".theme-big-selectbox").hide()
			});
			$(".theme-big-selectbox").click(function(e) {

				e.stopPropagation()
			});

			//发送推广申请
			$(".flowor-up").click(function() {
				layer.open({
					title: '发送推广申请',
					type: 1,
					area: '744px',
					skin: '',
					content: $('#flowor-up')
				});
			});

			//职位选择
			$(".job-data a").click(function() {
				if($(this).hasClass('cur')) {
					$(this).removeClass('cur');
				} else {
					$(this).addClass('cur')
				}
			})

			//确认到账浮层

			$(".confirm-receipt").click(function() {
				var self = this;
				confirmReceipt(self)
			});

			function confirmReceipt(ele) {
				var getHtml = $(ele).parent().html();
				var parent = $(ele).parent();
				$(ele).parent().children().remove();
				parent.append("<a href='javascript:void(0)' class='name-for-class plus-mar details-key'>详情</a>").queue(function() {
					$(this).children().click(function() {
						layer.open({
							title: "详情",
							type: "1",
							area: "682px",
							content: $("#details-layer-one")
						})
					});
					$(this).dequeue()
				});

				$(".sure-layer").addClass("cur");

				$(".cancel-sure").click(function() {
					parent.html(getHtml).queue(function(ele) {
						$(this).children(".confirm-receipt").click(function() {
							var elem = this;
							confirmReceipt(elem)
						});

						$(this).children(".cancel-key").click(function() {
							layer.open({
								title: "详情",
								type: "1",
								area: "682px",
								content: $("#details-layer-two")
							})
						});
						$(this).dequeue()
					});
					clearInterval(inter);
					$(".last-time").text("（5）");
					$(".sure-layer").removeClass("cur");
				});

				var i = 0;
				var t;
				var inter = setInterval(function() {
					++i;
					t = 5 - i;
					$(".last-time").text("（" + t + "）");
					if(t <= 0) {
						clearInterval(inter);
						$(".sure-layer").removeClass("cur");
						$(".last-time").text("（5）");
					}
				}, 1000);
				$(".sure-layer i").click(function() {
					clearInterval(inter);
					$(".sure-layer").removeClass("cur");
					$(".last-time").text("（5）");
				})
			}

