<script type="text/javascript">

    $(".adv-float a.set,.and-box").click(function () {
        layer.open({
            title: '添加广告',
            type: 1,
            area: '620px',
            skin: '',
            content: $('#unload-images')
        });
    });




    //主题广告hover事件
    $(".advertising-layer .advertising-floor .adv-box").hover(function () {
        $(this).find(".adv-float").fadeIn(200)
    },function () {
        $(this).find(".adv-float").fadeOut(200)
    });
    $(".adv-box .adv-float .del").click(function () {
        $(this).closest(".adv-box").remove()
    });



    //主题点击事件
    $(".advertising-floor .theme-box a").click(function () {
        $(".advertising-floor .theme-box a").removeClass("cur");
        $(this).addClass("cur")
    });

    //手机图片hover事件
    $(".advertising-layer .photo-mobel").hover(function () {
        $(".advertising-layer .mobel-float").fadeIn(200)
    },function () {
        $(".advertising-layer .mobel-float").fadeOut(200)
    })


</script>

